{
  "name": "the-ape-butler",
  "version": "1.0.0",
  "description": "A Master Thesis project exploring new possibilities to open doors by using speech. The project is nicknamed The Ape Butler.",
  "main": "server",
  "scripts": {
    "start": "nodemon --require babel-register --require babel-polyfill server/server.js",
    "stop": "killall node | exit 0",
    "watch": {},
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "jshint *.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-3",
      "stage-0"
    ],
    "sourceMaps": "inline"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apegroup/the-ape-butler.git"
  },
  "author": "Anders Berg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/apegroup/the-ape-butler/issues"
  },
  "homepage": "https://github.com/apegroup/the-ape-butler#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.0.15",
    "babel-preset-stage-3": "^6.17.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-cli": "^6.18.0",
    "babel-register": "^6.18.0"
  },
  "devDependencies": {
    "jshint": "latest",
    "nodemon": "^1.11.0",
    "onchange": "^3.2.0",
    "request": "^2.79.0"
  }
